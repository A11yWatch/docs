  "private": true,
  "scripts": {
    "docusaurus": "docusaurus --config docusaurus.config.ts",
    "start": "docusaurus start --host 0.0.0.0 --config docusaurus.config.ts",
    "build": "docusaurus build --config docusaurus.config.ts",
    "swizzle": "docusaurus swizzle --config docusaurus.config.ts",
    "deploy": "docusaurus deploy --config docusaurus.config.ts",
    "serve": "docusaurus serve --config docusaurus.config.ts",
    "clear": "docusaurus clear --config docusaurus.config.ts"
  },
  "dependencies": {
    "@docusaurus/core": "2.0.0-beta.14",
    "@docusaurus/preset-classic": "2.0.0-beta.14",
    "@mdx-js/react": "^1.6.22",
    "clsx": "^1.1.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.0-beta.14",
    "@tsconfig/docusaurus": "^1.0.2",
    "@types/react": "^17.0.3",
    "@types/react-helmet": "^6.1.0",
    "@types/react-router-dom": "^5.1.7",
    "typescript": "^4.2.3"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
